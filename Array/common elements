class Solution{
    public:
        vector<int> common_element(vector<int>v1,vector<int>v2)
    {
        vector<int> ans;
        map<int,int> mp;
        for(int i = 0;i<v1.size();i++){
            mp[v1[i]]++;
        }
        
        for(int i = 0;i<v2.size();i++){
            if(mp[v2[i]]){
                ans.push_back(v2[i]);
                mp[v2[i]]--;
            }
        }
        sort(ans.begin(),ans.end());
        return ans;
        // Your code here
    }
};
