void selectionSort(vector<int>&arr) {
   int temp;

    for(int i=0;i<arr.size();i++){

        int min =i;

        for(int j=i+1;j<arr.size();j++){

            if(arr[min]>arr[j]){

                temp=arr[min];

                arr[min]=arr[j];

                arr[j]=temp;

            }

        }

        

    }
}
