Custom Input
class Solution{
    public:
    // Function to find element in sorted array
    // arr: input array
    // N: size of array
    // K: element to be searche
    int searchInSorted(int arr[], int N, int k) 
    { 
        int i=0;
        for(int i=0;i<N;i++){
            if(arr[i]==k)
            return 1;
        }  
            return -1;
       
       // Your code here
       
    }
};
