class Solution
{
    public:
    //Function to reverse words in a given string.
    string reverseWords(string S) 
    { 
        string ans,temp;
        for(int i=0;i<s.size();i++){
            if(s[i]=='.'){
                ans='.'+temp+ans;
                temp="";
            }
            else{
                temp=temp+s[i];
            }
        }
        ans=temp+ans;
        return ans; 
        // code here 
    } 
};
