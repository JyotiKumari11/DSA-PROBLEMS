class Solution {
  public:
    // Function to count nodes of a linked list.
    bool searchKey(int n, struct Node* head, int key) {
        // Code here
        int pos=1;
        Node *temp=head;
        while(temp!=NULL){
        if(temp->data==key)
        return pos;
        else{
            pos++;
            temp=temp->next;
        }
    }
    return false;
}
